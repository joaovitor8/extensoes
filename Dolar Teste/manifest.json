{
  "manifest_version": 2,
  "name": "Cotação do Dólar",
  "version": "1.0",
  "description": "Exibe a cotação atual do dólar.",
  "permissions": ["https://api.exchangeratesapi.io/"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
